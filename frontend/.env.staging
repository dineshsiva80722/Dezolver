# Staging Environment Configuration

# API Configuration
VITE_API_URL=https://staging-api.techfolks.dev/api
VITE_WS_URL=wss://staging-api.techfolks.dev

# Application Configuration
VITE_APP_NAME=TechFolks (Staging)
VITE_APP_VERSION=1.0.0-staging
VITE_APP_DESCRIPTION=Competitive coding platform for developers - Staging Environment

# Authentication
VITE_AUTH_TOKEN_KEY=techfolks_staging_auth_token
VITE_REFRESH_TOKEN_KEY=techfolks_staging_refresh_token

# Features Configuration
VITE_ENABLE_GROUPS=true
